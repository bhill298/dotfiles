{
    "http.proxy": "http://proxy.sandia.gov:80",
    "http.proxyAuthorization": null,
    "vim.enableNeovim": true,
    "vim.neovimPath": "/usr/local/bin/nvim",
    "vim.vimrc.path": "~/.config/nvim/vimrc.vim",
    // https://github.com/tonsky/FiraCode/wiki/Installing
    "editor.fontFamily": "Fira Code",
    "vim.vimrc.enable": true,
    "redhat.telemetry.enabled": false,
    "git.openRepositoryInParentFolders": "never",
    "editor.autoClosingBrackets": "never",
    "workbench.editor.editorActionsLocation": "hidden",
    "editor.mouseWheelScrollSensitivity": 2,
    "workbench.colorTheme": "Default Light Modern",
    "python.analysis.typeCheckingMode": "basic",
    "vim.smartcase": false,
    "vim.ignorecase": false,
    "vim.useSystemClipboard": true,
    // https://github.com/tonsky/FiraCode/wiki/How-to-enable-stylistic-sets
    "editor.fontLigatures": "'ss08', 'cv24', 'ss05', 'ss03'",
    "window.titleBarStyle": "custom",
    "window.commandCenter": true,
    "terminal.integrated.sendKeybindingsToShell": true,
    "vim.textwidth": 120,
    "editor.rulers": [
        120
    ],
    "find-it-faster.general.killTerminalAfterUse": true,
    "find-it-faster.general.hideTerminalAfterSuccess": false,
    "find-it-faster.general.hideTerminalAfterFail": false,
    "html.autoClosingTags": false,
    "javascript.autoClosingTags": false,
    "typescript.autoClosingTags": false,
    "vim.highlightedyank.enable": true,
    "workbench.editor.titleScrollbarSizing": "large",
}
